<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="googleless11task.vibhuti.com.googlelesson11task.ui.download.DownloadActivity">
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="http://img.bhs4.com/BE/4/BE4AA7E888E32AB67B7ACC6E4D413DA63AA51C2C_large.jpg"
        android:id="@+id/edt_url"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Download"
        app:layout_constraintTop_toBottomOf="@+id/edt_url"
        android:id="@+id/btn_edt_download"/>

</android.support.constraint.ConstraintLayout>
